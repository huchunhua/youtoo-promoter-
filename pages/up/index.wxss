/**index.wxss**/
@import "/icon.wxss";
@import "/base-rpx.wxss";
page{
  background-color: #f4f4f4;
  text-align: center;
}
.item{
	background-color: #fff;
	width: 100%;
	height: 180rpx;
	border-radius: 10rpx;
}
checkbox{
	margin-top:40rpx;
}
checkbox .wx-checkbox-input{
      width: 22rpx;
      height: 22rpx;
      border-radius: 22rpx;
}
.logo{
	margin-top: 5rpx;
	width: 110rpx;
	height: 110rpx;
	border-right: 110rpx
}
.info{
	margin-top: 5rpx;
}
.add-icon{
	width: 18rpx;
	height: 20rpx;
}
.btn-group{
	position: fixed;
	bottom:0;
	left:0;
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	border-top: 1px solid #f4f4f4;
	background-color: #fff
}
checkbox-group{
	padding-bottom:120rpx;
}